extends ../layout/main

block variables
	- var title = 'Content';
	- 
		var pagelist = [
			{href: 'food-menu.html',name:'Food menu'},
			{href: 'food-menu-cards.html',name:'Food menu cards'},
			{href: 'product.html',name:'Popup order dish'},
			{href: 'delivery-areas.html',name:'Popup delivery areas'},
			{href: 'how-much.html',name:'Popup how much'},
			{href: 'typography.html',name:'Typography'},
		];


block content
	section
		.container
			h1 Content
			ul
				each page in pagelist
					li
						a(href=page.href,)= page.name



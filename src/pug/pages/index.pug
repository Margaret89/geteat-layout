extends ../layout/main

block variables
	- var title = 'Content';
	- 
		var pagelist = [
			{href: 'food-menu.html',name:'Food menu'},
			{href: 'food-menu-popup.html',name:'Food menu popup + order'},
			{href: 'food-menu-cards.html',name:'Food menu cards'},
			{href: 'side-menu-mobile.html',name:'Side menu mobile'},
			{href: 'login.html',name:'Login'},
			{href: 'typography.html',name:'Typography'},
		];

block content
	section
		.container
			h1 Content
			ul
				each page in pagelist
					li
						a(href=page.href,)= page.name


